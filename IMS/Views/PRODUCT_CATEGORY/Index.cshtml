@model IEnumerable<IMS.PRODUCT_CATEGORY>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_sidePanelPage.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CATEGORY.Category_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PRODUCT.Product_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PRODUCT_DETAILS.Product_Detail_ID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CATEGORY.Category_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PRODUCT.Product_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PRODUCT_DETAILS.Product_Detail_ID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Product_Category_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.Product_Category_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Product_Category_ID })
        </td>
    </tr>
}

</table>
